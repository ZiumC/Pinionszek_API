{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "Application": {
    "Properties": {
      "GeneralCatytegory": {
        "IdNeeds": 1,
        "IdWants": 2,
        "IdSavings": 3
      }
    },
    "User": {
      "Account": {
        "HideEmailPattern": "(?:(?:^|(?<=@))([^.@])|\\G(?!\\A))[^.@](?:([^.@])(?=[.@]))?",
        "Substitution": "$1*$2"
      }
    }
  }
}
